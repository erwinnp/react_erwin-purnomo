mutation addAnggaToKeterangan($id_anggota: Int, $nilai: Int, $pelajaran: String, $status: Boolean){
  insert_kampus_merdeka_keterangan(objects: {id_anggota: $id_anggota, nilai: $nilai, pelajaran: $pelajaran, status: $status}) {
    returning {
      id
      id_anggota
      pelajaran
      nilai
      status
    }
  }
}

